{
  "name": "proof-of-funds-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "node scripts/dev-servers.js",
    "dev:frontend": "npm run dev -w @proof-of-funds/frontend",
    "dev:backend": "npm run dev -w @proof-of-funds/backend",
    "build": "node scripts/prepare-zk-files.js && npm run build --workspaces",
    "prepare-zk": "node scripts/prepare-zk-files.js",
    "test": "npm run test --workspaces",
    "test:manual": "echo \"Running manual test - specify test script name\" && node",
    "test:manual:all": "for f in tests/manual/test-*.mjs; do echo \"Running $f\" && node $f; done",
    "compile": "npm run compile -w @proof-of-funds/contracts",
    "deploy:local": "npm run deploy:local -w @proof-of-funds/contracts",
    "deploy:amoy": "npm run deploy:amoy -w @proof-of-funds/contracts",
    "lint": "eslint \"packages/common/**/*.{js,jsx,ts,tsx}\" \"packages/frontend/**/*.{js,jsx,ts,tsx}\" \"scripts/**/*.js\" \"tests/**/*.js\"",
    "lint:fix": "eslint \"packages/common/**/*.{js,jsx,ts,tsx}\" \"packages/frontend/**/*.{js,jsx,ts,tsx}\" \"scripts/**/*.js\" \"tests/**/*.js\" --fix",
    "lint:fix-quotes": "eslint \"packages/common/**/*.{js,jsx,ts,tsx}\" \"packages/frontend/**/*.{js,jsx,ts,tsx}\" \"scripts/**/*.js\" \"tests/**/*.js\" --fix --rule '{\"quotes\": [\"error\", \"single\"]}'",
    "lint:file": "eslint",
    "lint:staged": "eslint $(git diff --staged --name-only --diff-filter=ACMRTUXB | grep -E '\\.(js|jsx|ts|tsx)$')",
    "lint:scripts": "eslint \"scripts/**/*.js\"",
    "lint:tests": "eslint \"tests/**/*.js\" \"tests/**/*.ts\""
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@types/snarkjs": "^0.7.9",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "babel-loader": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "browserify-fs": "^1.0.0",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^28.12.0",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "path-browserify": "^1.0.1",
    "prisma": "^4.16.2",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.3.4",
    "typescript": "^5"
  },
  "overrides": {
    "ethers": {
      "@proof-of-funds/frontend": "5.7.2",
      "@proof-of-funds/contracts": "6.10.0"
    }
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^6.0.1",
    "@google-cloud/storage": "^7.16.0",
    "ajv": "^8.17.1",
    "cookie-parser": "^1.4.7",
    "snarkjs": "^0.7.0"
  }
}
