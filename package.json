{
  "name": "proof-of-funds",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "hardhat compile --config hardhat.config.cjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy:local": "hardhat run scripts/deploy.js --config hardhat.config.cjs --network hardhat",
    "deploy:amoy": "hardhat run scripts/deploy.js --config hardhat.config.cjs --network polygon_amoy",
    "verify:amoy": "hardhat run scripts/verify.js --config hardhat.config.cjs --network polygon_amoy",
    "node": "hardhat node --config hardhat.config.cjs",
    "zk:build-circuits": "node lib/zk/scripts/build-circuits.mjs",
    "zk:test": "node lib/zk/tests/index.mjs",
    "test:zk": "node lib/zk/tests/index.mjs",
    "test:zk:all": "NODE_OPTIONS=--experimental-vm-modules jest lib/zk/__tests__ --config jest.config.cjs",
    "test:zk:snarkjs": "NODE_OPTIONS=--experimental-vm-modules jest lib/zk/__tests__/snarkjsLoader.test.js --config jest.config.cjs",
    "test:zk:api": "NODE_OPTIONS=--experimental-vm-modules jest lib/zk/__tests__/zkApiEndpoints.test.js --config jest.config.cjs",
    "test:zk:circuits": "NODE_OPTIONS=--experimental-vm-modules jest lib/zk/__tests__/circuits/ --config jest.config.cjs",
    "test:zk:performance": "NODE_OPTIONS=--experimental-vm-modules node lib/zk/scripts/run-performance-tests.mjs",
    "test:zk:security": "node lib/zk/scripts/run-security-test.js",
    "test:zk:security:attack": "NODE_OPTIONS=--experimental-vm-modules node lib/zk/scripts/run-security-test.js --test=attack",
    "test:zk:vuln": "NODE_OPTIONS=--experimental-vm-modules node lib/zk/scripts/run-implementation-vulnerability-check.js",
    "fix:zk": "node lib/zk/scripts/run-code-fixes.js",
    "fix:zk:modules": "node lib/zk/scripts/run-code-fixes.js --fixModules",
    "fix:zk:tests": "node lib/zk/scripts/run-code-fixes.js --fixTests",
    "fix:zk:dryrun": "node lib/zk/scripts/run-code-fixes.js --dryRun",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@ethersproject/keccak256": "^5.8.0",
    "@google-cloud/secret-manager": "^6.0.1",
    "@heroicons/react": "^2.2.0",
    "@openzeppelin/contracts": "^5.2.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/spl-token-registry": "^0.2.4574",
    "@solana/web3.js": "^1.98.0",
    "@walletconnect/web3-provider": "^1.8.0",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "circom": "^0.5.46",
    "circomlib": "^2.0.5",
    "crypto-js": "^4.2.0",
    "ethers": "^5.7.2",
    "hdkey": "^2.1.0",
    "next": "13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "snarkjs": "^0.7.5",
    "viem": "^1.19.11",
    "wagmi": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts-upgradeable": "^5.2.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.15",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "chai": "^5.2.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.54.0",
    "eslint-config-next": "13.5.6",
    "hardhat": "^2.22.19",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.16.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.3.1",
    "typescript": "^5"
  }
}